<a href="<%= root_url + 'videos/show/' + video.id.to_s %>">
<div class="col-xs-12 col-sm-12 col-md-4">
    <img class="video_thumb" src="<%= video.thumbnail %>" />
    <h4 class="video_title"><%= video.title %></h4>
    <% if session[:current_user] != nil %>
        <% if User.find(session[:current_user]).is_admin %>
            <%= link_to '[X]', 'videos/' + video.id.to_s + '/delete' %>
            <%= link_to '[make_featured]', root_url + 'videos/' + video.id.to_s + '/make_featured' %>
        <% end %>
    <% end %>
</div>
</a>