<h4><%= link_to question.title, root_url + 'questions/' + question.id.to_s %>
<% if session[:current_user] != nil %>
        <% if User.find(session[:current_user]).is_admin %>
            <%= link_to '[delete]', root_url + 'questions/' + question.id.to_s + '/delete' %>
        <% end %>
    <% end %>